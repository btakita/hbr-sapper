{
  "name": "menus-hbr-sapper",
  "version": "0.29.8",
  "description": "The next small thing in web development, powered by Svelte...with Hash Based Routing...customized for menus.com",
  "bin": {
    "sapper": "./dist/cli.js"
  },
  "type": "module",
  "exports": {
    "./package.json": {
      "import": "./package.json"
    },
    "./config/rollup.js": {
      "import": "./config/rollup.js"
    },
    "./config/webpack.js": {
      "import": "./config/webpack.js"
    }
  },
  "files": [
    "*.js",
    "webpack",
    "config",
    "sapper",
    "dist/**/*.js",
    "runtime/**/*.d.ts",
    "runtime/**/*.js",
    "runtime/**/*.mjs",
    "runtime/**/*.svelte"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "cheap-watch": "^1.0.4",
    "html-minifier": "^4.0.0",
    "http-link-header": "^1.0.5",
    "import-meta-resolve": "^2.0.3",
    "kleur": "^4.1.5",
    "node-fetch": "^3.2.6",
    "port-authority": "^2.0.1",
    "pretty-bytes": "^6.0.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-css-chunks": "^2.0.3",
    "sade": "1.8.1",
    "shimport": "^2.0.5",
    "source-map": "^0.7.4",
    "sourcemap-codec": "^1.4.8",
    "string-hash": "^1.1.3",
    "svelte": "^3.49.0",
    "webpack-format-messages": "3.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@sveltejs/eslint-config": "github:sveltejs/eslint-config#v5.4.0",
    "@types/cookie": "^0.5.1",
    "@types/mime": "^2.0.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.3",
    "@types/node-fetch": "^3.0.3",
    "@types/puppeteer": "^5.4.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "devalue": "^2.0.1",
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "mime": "^3.0.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "polka": "^0.5.2",
    "puppeteer": "^15.3.2",
    "rollup": "^2.76.0",
    "rollup-plugin-shim": "^1.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "sirv": "^2.0.2",
    "string-replace-async": "^3.0.2",
    "svelte-loader": "^3.1.3",
    "ts-node": "^10.8.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-modules": "^1.0.0"
  },
  "peerDependencies": {
    "svelte": "^3.49.0"
  },
  "scripts": {
    "test": "mocha --config=.mocharc.yml",
    "pretest": "npm run check",
    "build": "rimraf dist && tsc -p tsconfig.src.json && rollup -c && npm run strip_node_from_build",
    "strip_node_from_build": "cat ./runtime/server.js | sed -E 's/node://' > ./runtime/server.new.js && mv ./runtime/server.new.js ./runtime/server.js",
    "check": "rimraf dist && cross-env TS_CHECK_ENABLED=true rollup -c",
    "lint": "eslint '{src,runtime,test}/**/*.{ts,js,svelte}' && cd site && npm run lint",
    "dev": "rollup -cw"
  },
  "repository": "https://github.com/btakita/sapper#menus-hbr",
  "keywords": [
    "svelte",
    "isomorphic",
    "universal",
    "template",
    "express"
  ],
  "author": "Rich Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/btakita/sapper/issues"
  },
  "homepage": "https://github.com/btakita/sapper#menus-hbr#readme"
}
